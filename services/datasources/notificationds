dialect=mssql
driverClass=com.microsoft.sqlserver.jdbc.SQLServerDriver
url=jdbc:sqlserver://${dbserver_host};DatabaseName=${db_notification}
user=${dbserver_user}
pwd=${dbserver_pass}
